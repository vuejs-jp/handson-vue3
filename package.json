{
  "name": "handson-vue3-examples",
  "version": "0.2.0",
  "scripts": {
    "dev": "vite",
    "dev:v-for": "npm run dev -w=v-for",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "textlint": "textlint --format pretty-error",
    "textlint:docs": "textlint --format pretty-error docs/**"
  },
  "dependencies": {
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.1.0",
    "@vitejs/plugin-vue": "2.3.1",
    "@vue/eslint-config-prettier": "7.0.0",
    "eslint": "8.5.0",
    "eslint-plugin-vue": "8.4.1",
    "prettier": "2.5.1",
    "textlint": "12.1.0",
    "textlint-filter-rule-allowlist": "4.0.0",
    "textlint-rule-preset-vuejs-jp": "github:vuejs-jp/textlint-rule-preset-vuejs-jp",
    "vite": "^2.9.5"
  },
  "workspaces": [
    "examples/*"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "volta": {
    "node": "16.14.0"
  }
}
